rpm: dist
	for i in BUILD RPMS SRPMS SOURCES SPECS; do \
		j=$$PWD/RPMTOP/$$i; \
		if ! -d $$j; then \
			mkdir -p $$j || exit 1; \
		fi \
	done
	rpmbuild  --rmsource --define "_topdir $$PWD/RPMTOP" \
	  -ta $(PACKAGE)-$(JAS_VERSION).tar.gz
	rm -rf $$PWD/RPMTOP/BUILD/$(PACKAGE)-$(JAS_VERSION)
